{
	"name": "elwinar/elwinar",
	"description": "Elwinar's website",
	"keywords": ["website","elwinar","minimalist"],
	"license": "The Unlicense",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"elwinar/minit": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
